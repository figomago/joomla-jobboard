<?xml version="1.0" ?>
<joomfish type="contentelement">
  <name>Job Board - Email Templates</name>
  <author>figomago.wordpress.com</author>
  <version>1.0</version>
  <description>Definition for Email Templates</description>
  <copyright>2010 - 2012 figomago.wordpress.com.</copyright>
  <reference type="content">
  	<table name="jobboard_emailmsg">
